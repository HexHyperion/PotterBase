{
  "expo": {
    "name": "PotterBase",
    "slug": "PotterBase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.hexhyperion.PotterBase"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/HarryP.ttf",
            "./assets/fonts/Grenze-Regular.ttf",
            "./assets/fonts/Grenze-SemiBold.ttf",
            "./assets/fonts/Grenze-Bold.ttf",
            "./assets/fonts/Lato-Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f9cda0b1-f641-467c-81e3-cb92813f9e04"
      }
    }
  }
}
